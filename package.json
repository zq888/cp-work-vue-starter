{
  "name": "cp-work-vue-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "press:dev": "vuepress dev docs",
    "press:build": "vuepress build docs",
    "graphql-api": "vue-cli-service graphql-api",
    "run-graphql-api": "vue-cli-service run-graphql-api",
    "prod-graphql-api":
      "cross-env NODE_ENV=production vue-cli-service run-graphql-api",
    "build:electron": "vue-cli-service build:electron",
    "serve:electron": "vue-cli-service serve:electron"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.0.0",
    "apollo-client": "^2.0.1",
    "apollo-link": "^1.0.0",
    "apollo-link-context": "^1.0.5",
    "apollo-link-http": "^1.0.0",
    "apollo-link-persisted-queries": "^0.1.0",
    "apollo-link-ws": "^1.0.0",
    "apollo-upload-client": "^7.0.0-alpha.4",
    "apollo-utilities": "^1.0.1",
    "graphql": "^0.13.0",
    "netlify-auth-providers": "^1.0.0-alpha5",
    "subscriptions-transport-ws": "^0.9.5",
    "vue": "^2.5.13",
    "vue-apollo": "^3.0.0-alpha.1",
    "vue-class-component": "^6.0.0",
    "vue-property-decorator": "^6.0.0",
    "vue-router": "^3.0.1",
    "vuetify": "^1.0.0",
    "vuex": "^3.0.1",
    "vuex-class": "^0.3.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.107",
    "@types/nedb": "^1.8.5",
    "@types/ws": "^4.0.2",
    "@vue/cli-plugin-babel": "^3.0.0-beta.6",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.6",
    "@vue/cli-plugin-typescript": "^3.0.0-beta.6",
    "@vue/cli-service": "^3.0.0-beta.6",
    "@vue/eslint-config-prettier": "^3.0.0-beta.6",
    "@vue/eslint-config-typescript": "^3.0.0-beta.6",
    "electron": "^1.8.4",
    "electron-builder": "^20.8.1",
    "electron-webpack": "^1.13.0",
    "electron-webpack-ts": "^2.0.0",
    "eslint-plugin-graphql": "^1.5.0",
    "firebase": "^4.13.1",
    "graphql-tag": "^2.5.0",
    "lint-staged": "^6.0.0",
    "nedb": "^1.8.0",
    "vue-cli-plugin-apollo": "^0.7.7",
    "vue-cli-plugin-electron-builder": "^0.1.7",
    "vue-template-compiler": "^2.5.13",
    "vuepress": "^0.8.3"
  },
  "browserslist": ["> 1%", "last 2 versions", "not ie <= 8"],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": ["vue-cli-service lint", "git add"],
    "*.ts": ["vue-cli-service lint", "git add"],
    "*.vue": ["vue-cli-service lint", "git add"]
  },
  "electronWebpack": {
    "renderer": {
      "sourceDirectory": "src"
    }
  },
  "build": {
    "directories": {
      "output": "dist_electron"
    },
    "files": ["dist/**/*", "node_modules/**/*", "package.json"]
  }
}
